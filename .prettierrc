  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "endOfLine": "lf",
  "printWidth": 120,
  "bracketSpacing": true,
  "arrowParens": "always",
  "quoteProps": "as-needed",
  "useTabs": false,
  "jsxBracketSameLine": true,
  "proseWrap": "preserve"
